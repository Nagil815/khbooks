<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="bup">
	<insert id="ins" parameterType="dto.BookDTO">
		insert into book
		value(book_bno_seq.nextval, #{bno}, #{bname}, #{gno}, sysdate, #{bgrade,jdbcType=VARCHAR}, #{bview,jdbcType=VARCHAR}, #{bthumb}, #{binfo});
	</insert>
	
	<select id="list" parameterType="int" resultType="dto.BookDTO">
		select * from book
	</select>
		
</mapper>   











